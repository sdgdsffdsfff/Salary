<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.salary.entity" >
	<class name="Salary_item_expression" table="salary_item_expression">
		<id name="id">
			<generator class="increment"/>
		</id>
		
		<property name="name" />
		<property name="salary_item_id" />
		<property name="dynmaicsql" />
		<property name="comment" />
	</class>
</hibernate-mapping>
